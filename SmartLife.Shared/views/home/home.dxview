        <div style="width:100%;height:50%;">
            <div id="chart" data-bind="dxChart: chartOptions"></div>
        </div>
        <div style="width:100%;height:50%;" align="center">
            <div id="pie" data-bind="dxPieChart: piechartOptions"></div>
        </div>
    </div>-->
