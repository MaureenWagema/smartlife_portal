                                editing: {
                                    allowUpdating: false,
                                    mode: 'cell',
                                    allowAdding: false,
                                    allowDeleting: false,
                                },
                                summary: {
                                    texts: {
                                        avg: 'Avg={0}',
                                        avgOtherColumn: 'Avg of {1} is {0}',
                                        count: 'Total Agents={0}',
                                        max: 'Max={0}',
                                        maxOtherColumn: 'Max of {1} is {0}',
                                        min: 'Min={0}',
                                        minOtherColumn: 'Min of {1} is {0}',
                                        sum: 'Total={0}',
                                        sumOtherColumn: 'Sum of {1} is {0}'
                                    },
                                    totalItems: [
                                        {
                                            column: 'names',
                                            summaryType: 'count'
                                        }
                                    ],
                                    groupItems: [
                                        {
                                            column: 'name',
                                            summaryType: 'count',
                                            showInGroupFooter: false,
                                            showInColumn: 'name',
                                            displayFormat: ' {0} Agents',
                                            alignByColumn: true
                                        }
                                    ]
                                },
                                paging: {
                                    enabled: false,
                                    pageSize: 20
                                },
                                pager: {
                                    showPageSizeSelector: true,
                                    showInfo: true
                                },
                                headerFilter: { visible: false },
                                filterRow: {
                                    visible: false,
                                    applyFilter: 'auto',
                                },
                            }"></div>
